{
    "name": "css2react",
    "displayName": "css2react",
    "description": "Convert selection between CSS and React inline style syntax. Works both ways.",
    "icon": "images/icon.png",
    "version": "0.0.9",
    "publisher": "gottfired",
    "homepage": "https://github.com/gottfired/css2react/blob/master/README.md",
    "repository": {
        "type": "git",
        "url": "https://github.com/gottfired/css2react.git"
    },
    "engines": {
        "vscode": "^1.11.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:extension.css2react"
    ],
    "main": "./out/src/extension",
    "contributes": {
        "commands": [
            {
                "command": "extension.css2react",
                "title": "CSS to React"
            }
        ],
        "keybindings": [
            {
                "command": "extension.css2react",
                "key": "ctrl+alt+c",
                "mac": "cmd+alt+c",
                "when": "editorTextFocus"
            }
        ],
        "configuration": {
            "type": "object",
            "title": "CSS to React",
            "properties": {
                "css2react.singleQuotes": {
                    "type": "boolean",
                    "default": false,
                    "markdownDescription": "Use single instead of double quotes"
                }
            }
        }
    },
    "scripts": {
        "vscode:prepublish": "tsc -p ./",
        "compile": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.0.3",
        "vscode": "^1.0.0",
        "mocha": "^2.3.3",
        "@types/node": "^6.0.40",
        "@types/mocha": "^2.2.32"
    }
}
